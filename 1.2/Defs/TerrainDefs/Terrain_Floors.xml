  <!-- ##### Plastic Tile ##### -->
  <TerrainDef Name="PlasticTileBase" ParentName="FloorBase" Abstract="True">
    <description>Plastic planks cutted into big squares. Gives that feeling of modernity and its extra-smooth surface allows for a small bonus to cleanliness.</description>
    <texturePath>Terrain/Surfaces/VMEu_PlasticTile</texturePath>
    <statBases>
      <WorkToBuild>95</WorkToBuild>
      <Flammability>0.35</Flammability>
	  <Beauty>0.1</Beauty>
	  <Cleanliness>0.1</Cleanliness>
    </statBases>
    <costList>
      <VMEu_PlasticPlanks_TierOne>3</VMEu_PlasticPlanks_TierOne>
    </costList>
    <constructEffect>ConstructWood</constructEffect>
	<designatorDropdown>VMEu_PlasticTileDropdownMenu</designatorDropdown>
	<researchPrerequisites>
      <li>VMEu_PlasticManufacture</li>
    </researchPrerequisites>
  </TerrainDef>
  
