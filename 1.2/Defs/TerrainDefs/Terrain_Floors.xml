  <!-- ##### Asphalt Road ##### -->
  <TerrainDef ParentName="FloorBase">
    <defName>VMEu_AsphaltRoad</defName>
    <label>asphalt</label>
    <description>Rough asphalt road. It is very quick to build and it it very easy to travel through, it also does look quite neat.</description>
    <texturePath>Terrain/Surfaces/Concrete</texturePath>
	<color>(86, 86, 86)</color>
    <edgeType>Hard</edgeType>
    <renderPrecedence>90</renderPrecedence>
    <statBases>
      <WorkToBuild>95</WorkToBuild>
    </statBases>
    <costList>
      <VMEu_AsphaltMix>1</VMEu_AsphaltMix>
    </costList>
    <researchPrerequisites>
      <li>BiofuelRefining</li>
    </researchPrerequisites>
  </TerrainDef>

	

