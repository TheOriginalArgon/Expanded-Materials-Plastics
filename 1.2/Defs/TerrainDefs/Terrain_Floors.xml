<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- ##### Plastic Decorative Floors ##### -->

<!-- ##### Plastic Mosaic ##### -->
<TerrainDef Name="PlasticMosaicBase" ParentName="FloorBase" Abstract="True">
    <description>Plastic planks cutted into small mosaics. Pretty nice to see, can be colored in different ways to achieve the perfect look.</description>
    <texturePath>Terrain/Surfaces/VMEu_PlasticMosaic</texturePath>
    <statBases>
      <WorkToBuild>85</WorkToBuild>
      <Flammability>0.35</Flammability>
	  <Beauty>0.3</Beauty>
    </statBases>
    <costList>
      <VMEu_PlasticPlanks_TierOne>2</VMEu_PlasticPlanks_TierOne>
    </costList>
    <constructEffect>ConstructWood</constructEffect>
	<designatorDropdown>VMEu_PlasticMosaicDropdownMenu</designatorDropdown>
	<researchPrerequisites>
      <li>VMEu_PlasticManufacture</li>
    </researchPrerequisites>
  </TerrainDef>
  
  <TerrainDef ParentName="PlasticMosaicBase">
    <defName>VMEu_PlasticMosaic_Beige</defName>
    <label>beige plastic mosaics</label>
	<renderPrecedence>250</renderPrecedence>
    <color>(186, 177, 149)</color>
  </TerrainDef>
  
