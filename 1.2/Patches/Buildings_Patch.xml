<?xml version="1.0" encoding="utf-8" ?>
<Patch>

		<Operation Class="PatchOperationAdd">
			<xpath>/Defs/ThingDef[not(@Name = "Wall") and stuffCategories/li[text()="Metallic"] and stuffCategories/li[text()="Woody"] and stuffCategories/li[text()="Stony"]]/stuffCategories</xpath>
				<value>
					<li>VMEu_PlasticStuffCategory</li>
				</value>
		</Operation>
		
		<Operation Class="PatchOperationAdd">
			<xpath>/Defs/ThingDef [@Name = "Wall"]/stuffCategories</xpath>
				<value>
					<li>VMEu_PlasticPlanksStuffCategory</li>
				</value>
		</Operation>
		
</Patch>